
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.IndexPage>
<!-- Page Content-->
<section>
    <div class="container px-4 px-lg-5 mh-100 text-white">
        <div class="row gx-4 gx-lg-5 mx-auto ">
            <div class="col-lg-6">
                @if (!string.IsNullOrEmpty(@Model.Header) )
				{
				 <h1 class="mt-5 ">@Model.Header</h1>
				}	
                @if (!string.IsNullOrEmpty(@Model.Description) ) 
				{
					<p>@Model.Description</p>
				}
            </div>
        </div>
        
    </div>
</section>