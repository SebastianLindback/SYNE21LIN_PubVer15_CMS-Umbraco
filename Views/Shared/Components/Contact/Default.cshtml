@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<List<CMS.ContactDTO>>



<section>
    <div class="container py-5 px-lg-5 text-white">
        <div class="row gx-4 py-5 gx-lg-5 mx-auto ">
            @await Html.PartialAsync("ContactPartial", new CMS.ContactDTO())
        </div>
        <div class="row gx-4  py-5 gx-lg-5 mx-auto">
            @if (Model.Any())
            {
                <table class="table table-dark ">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Message</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var data in Model)
                        {
                            <tr>
                                <td>@data.Name</td>
                                <td>@data.Email</td>
                                <td>@data.Message</td>
                            </tr>
                        }
                    </tbody>
                </table>
            }            
        </div>
    </div>
</section>



































































































































}